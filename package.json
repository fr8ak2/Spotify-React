{
	"name": "spotify",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint:all": "prettier --write {pages,src/components,src/context,src/shared,src/layout,src/hooks,src/utils} && next lint --fix",
		"prepare": "husky install"
	},
	"dependencies": {
		"clsx": "^2.0.0",
		"next": "14.0.3",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@svgr/webpack": "^8.1.0",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"eslint": "^8",
		"eslint-config-next": "14.0.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.1.0",
		"path": "^0.12.7",
		"prettier": "^3.1.0",
		"sass": "^1.69.5",
		"typescript": "^5",
		"url": "^0.11.3"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"volta": {
		"node": "20.10.0",
		"yarn": "1.22.19"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,scss}": [
			"prettier --write"
		],
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	}
}
